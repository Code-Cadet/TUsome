{
    "builds": [
      {
        "src": "tusome/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": {
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "tusome/wsgi.py"
      }
    ]
  }
  